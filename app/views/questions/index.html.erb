<%= link_to "Volver", :back , :class => "btn btn-inverse" %><br><br>
<h1>Lista de preguntas</h1>

<p>
<%= link_to 'Hacer una pregunta', new_question_path %>
</p>

<% @questions.each do |question| %>
  <h3> <%= link_to question.title, question %></h3>
  Hace <%= time_ago_in_words question.created_at %>
  por <%= link_to question.user.username, question.user %>
  <p>
  <%= link_to 'Show', question %> |
  <%= link_to 'Comentar'%> |
  <%= link_to 'Responder'%>
  </p>
<% end -%>

